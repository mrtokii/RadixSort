<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane minHeight="300.0" minWidth="400.0" prefHeight="1080.0" prefWidth="1920.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="etu8.room117.gui.Controller">
    <children>
        <VBox prefHeight="600.0" prefWidth="1000.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <HBox minHeight="50.0" prefHeight="50.0" prefWidth="200.0">
                    <children>
                        <Label minHeight="35.0" minWidth="130.0" prefWidth="130.0" text="Введите числа: ">
                            <font>
                                <Font size="18.0" />
                            </font></Label>
                        <TextField fx:id="Text_Field" minWidth="50.0" prefWidth="1780.0">
                            <font>
                                <Font size="18.0" />
                            </font></TextField>
                        <Button fx:id="EnterDataButton" minWidth="120.0" mnemonicParsing="false" onAction="#onEnterDataClicked" prefWidth="120.0" text="Ввести данные">
                            <HBox.margin>
                                <Insets left="10.0" top="2.0" />
                            </HBox.margin>
                            <font>
                                <Font size="14.0" />
                            </font>
                        </Button>
                    </children>
                    <padding>
                        <Insets left="10.0" right="10.0" top="10.0" />
                    </padding>
                </HBox>
                <Label text="Чтобы заполнить случайными числами, введите необходимые данные:">
                    <font>
                        <Font size="18.0" />
                    </font>
                    <VBox.margin>
                        <Insets left="10.0" right="10.0" />
                    </VBox.margin>
                    <padding>
                        <Insets top="20.0" />
                    </padding></Label>
                <HBox minHeight="37.0" minWidth="37.0" prefHeight="37.0" prefWidth="200.0">
                    <children>
                        <Label minHeight="35.0" minWidth="240.0" prefWidth="240.0" text="Генерировать числа от 0 до:">
                            <font>
                                <Font size="18.0" />
                            </font>
                        </Label>
                        <TextField fx:id="TextFieldTill" minWidth="100.0" prefWidth="100.0">
                            <font>
                                <Font size="18.0" />
                            </font></TextField>
                        <Label minHeight="35.0" minWidth="160.0" prefWidth="160.0" text="Количество чисел:">
                            <font>
                                <Font size="18.0" />
                            </font>
                            <HBox.margin>
                                <Insets left="40.0" />
                            </HBox.margin>
                        </Label>
                        <TextField fx:id="TextFieldNum" minWidth="100.0" prefWidth="100.0">
                            <font>
                                <Font size="18.0" />
                            </font></TextField>
                    </children>
                    <padding>
                        <Insets left="10.0" right="10.0" />
                    </padding>
                </HBox>
                <Button fx:id="FillingButton" minWidth="100.0" mnemonicParsing="false" onAction="#onFillingButtonClicked" text="Заполнить">
                    <font>
                        <Font size="14.0" />
                    </font>
                    <VBox.margin>
                        <Insets left="10.0" />
                    </VBox.margin>
                </Button>
                <HBox alignment="CENTER" prefHeight="40.0" prefWidth="200.0">
                    <children>
                        <Label fx:id="DigitLabel" minWidth="100.0" prefWidth="300.0">
                            <font>
                                <Font size="18.0" />
                            </font>
                        </Label>
                    </children>
                    <VBox.margin>
                        <Insets top="10.0" />
                    </VBox.margin>
                    <padding>
                        <Insets left="10.0" right="10.0" />
                    </padding>
                </HBox>
                <ScrollPane fx:id="scrollAreaArray" prefHeight="200.0" prefWidth="200.0">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="200.0" prefWidth="200.0" />
                    </content>
               <VBox.margin>
                  <Insets left="10.0" right="10.0" />
               </VBox.margin>
                </ScrollPane>
                <ScrollPane fx:id="scrollPaneLow" prefHeight="460.0" prefWidth="200.0">
                    <VBox.margin>
                        <Insets left="10.0" right="10.0" />
                    </VBox.margin>
                    <content>
                        <HBox fx:id="MainHBox" />
                    </content></ScrollPane>
                <HBox alignment="CENTER" minHeight="50.0" prefHeight="50.0">
                    <children>
                        <Button fx:id="NextStepButton" minWidth="130.0" mnemonicParsing="false" onAction="#onNextStepButtonClicked" prefWidth="130.0" text="Следующий шаг">
                            <font>
                                <Font size="14.0" />
                            </font></Button>
                  <Button minWidth="150.0" mnemonicParsing="false" onAction="#onPreviousStepButtonClicked" text="Предыдущий шаг">
                     <font>
                        <Font size="14.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="60.0" />
                     </HBox.margin>
                  </Button>
                        <Button fx:id="FinishSorting" minWidth="170.0" mnemonicParsing="false" onAction="#onFinishSortingButtonClicked" prefWidth="170.0" text="Завершить сортировку">
                            <font>
                                <Font size="14.0" />
                            </font>
                            <HBox.margin>
                                <Insets left="60.0" />
                            </HBox.margin>
                        </Button>
                        <Button fx:id="ClearButton" minWidth="100.0" mnemonicParsing="false" onAction="#onClearButtonClicked" text="Сброс">
                            <font>
                                <Font size="14.0" />
                            </font>
                            <HBox.margin>
                                <Insets left="200.0" right="60.0" />
                            </HBox.margin>
                        </Button>
                        <Button fx:id="ExitButton" minWidth="100.0" mnemonicParsing="false" onAction="#onExitButtonClicked" text="Выход">
                            <font>
                                <Font size="14.0" />
                            </font>
                        </Button>
                    </children>
                    <VBox.margin>
                        <Insets bottom="10.0" top="20.0" />
                    </VBox.margin>
                </HBox>
            </children>
        </VBox>
    </children>
</AnchorPane>
